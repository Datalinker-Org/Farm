{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding-Id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Internal identifier in the source system"
        },
        "identifiers": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding-Identifier",
            "@type": "xsd:string",
            "type": "array",
            "items": {
                "type": "string"
            },
            "@container": "@set",
            "dldescription": "Holding Identifiers using a URN-based identification string containing namespace and unique identifier within the namespace. Used as a unique identifier. Eg. urn:nzl:pri:holding:AgriBase:12345, urn:nzl:pri:holding:FOL_ID:CL:67645"
        },
        "name": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding-Name",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Name of the holding"
        },
        "sites": {
            "@id": "https://www.datalinker.org/context/FarmProfileSite",
            "@type": "https://www.datalinker.org/contexts/2.0/FarmProfileSite.jsonld",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Array of sites in the holding"
        },
        "topography": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding-topography",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Rolling, Easy hill, Steep hill (this enumeration is defined by Overseer). Can be different for other systems"
        },
        "organic": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding-Name",
            "@type": "xsd:boolean",
            "type": "boolean",
            "dldescription": "Indicates whether holding is organic or not."
        },
        "meta":{
            "@id": "https://www.datalinker.org/context/ICARObjectMetaData",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARObjectMetaData",
            "type": "object",
            "dldescription": "Any metadata for the object.  Most likely to use 'modified'"
        }
    }
}