{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "blockId": {
      "@id": "[blockId](#blockId)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Identifier for the block"
    },
    "blockName": {
      "@id": "[blockName](#blockName)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Name of the block"
    },
    "effectiveArea": {
      "@id": "[effectiveArea](#effectiveArea)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Block effective area in hectares"
    },
    "ungrazeableArea": {
      "@id": "[ungrazeableArea](#ungrazeableArea)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Block ungrazeable area in hectares"
    },
    "blockOwnership": {
      "@id": "[blockOwnership](#blockOwnership)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Values can be 'Owned' or 'Leased'"
    },
    "topography": {
      "@id": "[topography](#topography)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Values can be Flat,Rolling,Easy hill, Steep hill"
    },
    "blockType": {
      "@id": "[blockType](#blockType)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Pature, Support or Crop"
    },
    "dtStart": {
      "@id": "[dtStart](#dtStart)",
      "@type": "xsd:date",
      "type": "string",
      "dldescription": "Date started operating for specific paddock type"
    },
    "dtEnd": {
      "@id": "[dtEnd](#dtEnd)",
      "@type": "xsd:date",
      "type": "string",
      "dldescription": "Date ended(ceased) operating for specific paddock type"
    },
    "pastureType": {
      "@id": "[pastureType](#pastureType)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Enumeration: Ryegrass/white clover, Browntop, Unimproved/tussock grasslands, Summer C4 (paspalum) pastures, C4 (kikuyu) pastures, Lucerne, Grass only (enumeration defined by Overseer)"
    },
    "cloverLevel": {
      "@id": "[cloverLevel](#cloverLevel)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Annual average clover content (as a proportion of pasture dry matter) where fertiliser N inputs are not applied. Enumeration defined by Overseer. Enumeration: Very low, Low, Medium, High, Very high"
    },
    "pastureUtilisation": {
      "@id": "[pastureUtilisation](#pastureUtilisation)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Expressed in percentage"
    },
    "avgPastureNConc": {
      "@id": "[avgPastureNConc](#avgPastureNConc)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Expressed in percentage"
    },
    "roadAddress": {
      "@id": "[roadAddress](#roadAddress)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Road address of the block"
    },
    "stockTypes": {
      "@id": "[stockTypes](#stockTypes)",
      "@type": "xsd:string",
      "type": "array",
      "items": { "type": "string" },
      "@container": "@set",
      "dldescription": "Dairy Cows [Cows], Dairy Replacements [Young Stock], Pigs, Sheep, Beef Cattle [Cattle], Deer"
    },
    "numberOfStock": {
      "@id": "[numberOfStock](#numberOfStock)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Number of animals"
    },
    "dtStockCome": {
      "@id": "'[dtStockCome](#dtStockCome)",
      "@type": "xsd:date",
      "type": "string",
      "dldescription": "Date when the animals came in"
    },
    "dtStockLeave": {
      "@id": "'[dtStockLeave](#dtStockLeave)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Date when the animals moved out"
    },
    "areaHayPerSilageHarvested": {
      "@id": "'[areaHayPerSilageHarvested](#areaHayPerSilageHarvested)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Expressed in Hectares"
    },
    "destinationHaySilage": {
      "@id": "'[destinationHaySilage](#destinationHaySilage)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Fed on support block, Milking area or Elsewhere (defined by Overseer)"
    },
    "runoff": {
      "@id": "'[runoff](#runoff)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "True if runoff block"
    },
    "pastureSpecies": {
      "@id": "'[pastureSpecies](#pastureSpecies)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Plant species names as defined in http://www.feedipedia.org/content/feeds?category=15965"
    },
    "croppingArea": {
      "@id": "'[croppingArea](#croppingArea)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Area planted in crop. In Hectares"
    }
  }
}
