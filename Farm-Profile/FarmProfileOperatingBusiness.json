{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/FarmProfileOperatingBusiness-Id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Internal identifier in the source system"
        },
        "owners": {
            "@id": "http://schema.org/Person",
            "@type": "http://schema.org/Person.jsonld",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "List of owners of the farm"
        },
        "holding": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding",
            "@type": "http://www.datalinker.org/contexts/2.0/FarmProfileHolding.jsonld",
            "type": "object",
            "dldescription": "Provides details of the holding and the sites within it"
        },
        "organization": {
            "@id": "http://schema.org/Organization",
            "@type": "http://schema.org/Organization.jsonld",
            "type": "object",
            "dldescription": "Organisation that owns or controls the farm"
        },
        "herds": {
            "@id": "https://www.datalinker.org/context/FarmProfileHerd",
            "@type": "http://www.datalinker.org/contexts/2.0/FarmProfileHerd.jsonld",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Array of herds on the farm"
        },
        "supplyAgreements": {
            "@id": "https://www.datalinker.org/context/FarmProfileSupplyAgreement",
            "@type": "http://www.datalinker.org/contexts/2.0/FarmProfileSupplyAgreement.jsonld",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Indicates the products and organisations that the farm supplies to"
        },
        "type": {
            "@id": "https://www.datalinker.org/context/FarmProfileHolding-Type",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Primary or secondary operating business type."
        },
        "meta":{
            "@id": "https://www.datalinker.org/context/ICARObjectMetaData",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARObjectMetaData",
            "type": "object",
            "dldescription": "Any metadata for the object.  Most likely to use 'modified'"
        }
    }
}